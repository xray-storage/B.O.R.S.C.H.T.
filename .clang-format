BasedOnStyle: Webkit
Standard: Cpp11
BreakBeforeBraces: Custom
BraceWrapping:
  AfterClass: true
  AfterFunction: true
ColumnLimit: 120
SortIncludes: true
IncludeCategories:
  - Regex: '\"stdafx\.h'
    Priority: -1
  - Regex: '\"pch_script\.h'
    Priority: -1
IndentPPDirectives: AfterHash
AlignConsecutiveDeclarations:
  Enabled: true
  AcrossEmptyLines: true
  AcrossComments: true